{
  "public": true,
  "functions": {
    "app/api/stripe/webhooks/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/content/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*\\.(?:jpg|jpeg|png|gif|webp|svg).*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
} 